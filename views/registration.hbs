                               value="{{listtoPort.faxNumber}}" required>

                        <label id="faxNumberLabel" style="color: red;visibility: hidden;"></label>

                    </div>
                </div>

                <div class="panel-body">

                    <div class="form-group col-md-6">
                        <label>What sector do you work?</label>
                        <select id="sectorSelectjs" class="form-control" name="workSector" required>


                            <option value="">-Select a value-</option>
                            <option value="Academic">Academic</option>
                            <option value="Government">Government</option>
                            <option value="Industry">Industry</option>
                            <option value="Other">Other</option>


                        </select>
                        <label id="workSectorLabel" style="color: red;visibility: hidden;"></label>

                    </div>
                    <div class="form-group col-md-6">
                        <label>What is your primary job function?</label>
                        <select id="jobFunctionSelectjs" class="form-control" name="jobFunction" required>


                            <option value="">-Select a value-</option>
                            <option value="Academic/Researcher">Academic/Researcher</option>
                            <option value="Engineer/Developer">Engineer/Developer</option>
                            <option value="System/Network Administrator">System/Network Administrator</option>
                            <option value="Manager/Director">Manager/Director</option>


                        </select>
                        <label id="jobFunctionLabel" style="color: red;visibility: hidden;"></label>

                    </div>


                </div>

                <div class="panel-body">
                    <div class="row">
                        <div class="form-check col-md-3">
                            <label>Are you a full time student?</label>
                        </div>
                    </div>

                    <div class="row">
                        <div class="form-check col-md-1 col-xs-2">
                            <label class="form-check-label">
                                <input class="form-check-input" type="radio" name="exampleRadios" id="Radios1"
                                       value="yes" required>
                                Yes
                            </label>
                        </div>
                        <div class="form-check">
                            <label class="form-check-label col-xs-2">
                                <input class="form-check-input" type="radio" name="exampleRadios" id="Radios2"
                                       value="no" required>
                                No
                            </label>
                        </div>

                    </div>
                    <label id="exampleRadiosLabel" style="color: red;visibility: hidden;"></label>

                </div>
                <hr>
                <div class="panel-body text-center">

                    <label style="text-decoration: underline">Captcha</label><br>
                    <label id="captchaLabel" style="color: red;visibility: hidden;"></label><br>
                    <div id="rc-imageselect" class="g-recaptcha" style="display: inline-block;"
                         data-sitekey="{{captchaClientKey}}" data-size="compact"></div>

                    <!--6LeIxAcTAAAAAJcZVRqyHh71UMIEGNQ_MXjiZKhI-->
                </div>


                <div class="row text-center">
                    <button type="submit" class="btn btn-primary btn-lg">Sign UP</button>

                </div>
                <br>
                <br>
            </form>


            {{#if existingdata}}

                <script>
                    $(document).ready(function () {

                        $('#countrySelectjs').val('{{listtoPort.countryName}}');
                        $('#sectorSelectjs').val('{{listtoPort.workSector}}');
                        $('#jobFunctionSelectjs').val('{{listtoPort.jobFunction}}');

                        var checkRadio = "{{listtoPort.exampleRadios}}";

                        if (checkRadio == "yes") {
                            document.getElementById("Radios1").checked = true;
                        } else {
                            document.getElementById("Radios2").checked = true;
                        }


                    });

                </script>
            {{/if}}



            <script>
                var temp2;
            </script>
            <!--<button onclick="checktempinarr()">df</button>-->
            {{# if hasError}}
                {{#each messages}}
                    <script>

                        document.getElementById('{{param}}Label').style.visibility = "visible";

                        temp2 = document.getElementById('{{param}}Label').innerHTML;
                        document.getElementById('{{param}}Label').innerHTML = temp2 + "<br>{{msg}}";
                        <!--console.log(document.getElementById('{{param}}Label').innerHTML);-->


                    </script>

                {{/each}}



            {{/if}}
        {{/if}}


