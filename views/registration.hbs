            <form class="panel panel-default" action='/page/registerForm' method="post" id="registerForm">


                <div class="panel-body">
                    <div class="form-group col-md-6">
                        <label>Email</label>
                        <input type="email" class="form-control" id="emailAddress" placeholder="Email"
                               value="{{listtoPort.emailadd}}"
                               name="emailAddress" onfocusout="myFunction()" required>
                        <label id="emailAddressLabel" style="color: red;visibility: hidden;"></label>
                        <label id="existinguserFound" style="visibility: hidden;"></label>


